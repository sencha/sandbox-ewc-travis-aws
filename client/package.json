{
  "name": "my-app",
  "version": "0.0.1",
  "description": "my-app description for Ext-Web-Components app MyApp",
  "keywords": [
    "ExtWebComponents"
  ],
  "license": "ISC",
  "scripts": {
    "build": "npm run clean && npm run webpackprod && npm run gzip && npm run http",
    "clean": "rimraf build",
    "dev": "webpack-dev-server --env.browser=yes --env.verbose=no",
    "gzip": "npm run gzipbuild && npm run gzipext && npm run gzipresources",
    "gzipbuild": "npx gzip build/*.js",
    "gzipext": "npx gzip build/ext/*.js",
    "gzipresources": "npx gzip build/resources/*.js",
    "http": "npx http-server build -g -o",
    "replace": "npx replace-in-file /.js/g .js.gz build/index.html --isRegex",
    "start": "npm run dev",
    "webpackprod": "cross-env webpack --env.environment=production --env.treeshake=yes --env.basehref=/"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/plugin-proposal-function-sent": "^7.8.3",
    "@babel/plugin-proposal-json-strings": "^7.8.3",
    "@babel/plugin-proposal-numeric-separator": "^7.8.3",
    "@babel/plugin-proposal-throw-expressions": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@sencha/ext": "~7.2.0",
    "@sencha/ext-modern": "~7.2.0",
    "@sencha/ext-modern-theme-material": "~7.2.0",
    "@sencha/ext-modern-treegrid": "~7.2.0",
    "@sencha/ext-web-components-modern": "~7.2.0",
    "@sencha/ext-webpack-plugin": "~7.2.0",
    "@webcomponents/webcomponentsjs": "^2.4.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "base-href-webpack-plugin": "~2.0.0",
    "compression-webpack-plugin": "^3.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "fs-extra": "^8.1.0",
    "gzip-cli": "^1.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.12.1",
    "node-sass": "^4.13.1",
    "portfinder": "^1.0.25",
    "replace-in-file": "^5.0.2",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.4",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
